:@template"HL/* fonts */
@import url(http://fonts.googleapis.com/css?family=Alegreya+SC:400,700,900);
@import "compass/css3/box-sizing";
$base-font:   "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
$fancy-font:   'Alegreya SC', Georgia, "Times New Roman", "DejaVu Serif", serif serif;

/* colors */
$brown:         #46352f;
$brown-light:   rgba(146,100,84,0.1);
$blue:          #454d79;
$blue-light:    #5b6392;
$green:         #23533b;
$green-light:   #457951;
$white:         #ffffff;
$black:         #1a1a1a;
$orange:        #cc5500;
$orange-light:  #d47e40;
$orange-opaque: rgba(212,125,63,0.25);

/* other variables */
$radius:      2px;

/* breakpoints */
$break-small: 400px;

/* hover style background position change */
@-webkit-keyframes bp { /* Safari and Chrome */
  from {
    background-position-y: 0;
  }
  to {
    background-position-y: -95px;
  }
}
@-moz-keyframes bp { /* Safari and Chrome */
  from {
    background-position: inherit 0;
  }
  to {
    background-position: inherit -95px;
  }
}
@-o-keyframes bp { /* Safari and Chrome */
  from {
    background-position-y: 0;
  }
  to {
    background-position-y: -95px;
  }
}
@keyframes bp { /* Safari and Chrome */
  from {
    background-position-y: 0;
  }
  to {
    background-position-y: -95px;
  }
}

/* the basics */
body, html {
  background: $brown url('../images/main_bg.jpg') no-repeat 50% -10em;
  background-attachment: fixed;
  color: $white;
  font-family: $base-font;
  font-weight: 400;
  font-size: 100%;
}
a:link,
a:visited {
  border-bottom: 1px solid transparent;
  color: $orange;
  text-decoration: none;
  -moz-transition: border-bottom 0.5s ease;
  -o-transition: border-bottom 0.5s ease;
  -webkit-transition: border-bottom 0.5s ease;
  transition: border-bottom 0.5s ease;
}
a:hover,
a:focus,
a:active {
  border-bottom: 1px solid $white;
  color: $orange-light;
}
.btn-container,
.form-actions {
  font-size: 1.25em;
  text-align: center;
  
  .btn,
  .form-submit {
  background: $orange;
  border: none;
  border-radius: $radius;
  box-shadow: 1px 1px 5px rgba(0,0,0,0.25);
  color: $white;
  margin: 0 0.5em;
  padding: 0.5em 2em;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  -webkit-transition: all 0.5s ease;
  transition: all 0.5s ease;
    
    &:hover,
    &:active, 
    &:focus {
      background: $orange-light;
    }
  }
}
.node-webform {
  text-align: center;
}
.form-item {
  @include box-sizing(border-box);
  
  margin: 0 auto 1em;
  max-width: 500px;
  padding: 0 1em;
  width: 100%;
  
  &.form-type-checkbox,
  &.form-type-radios {
    text-align: left;
  }
  
  label {
    text-align: left;
  }
  
  input,
  textarea {
    border: 1px solid #ccc;
    border-radius: $radius;
    color: $brown;
    padding: 0.5em;
    width: 100%;
    
    &.form-checkbox {
      width: auto;
    }
  }
}
.comment-form {
  .form-item {
    margin: 0 0 1em;
  }
  .filter-wrapper {
    display: none;
  }
}
::-webkit-input-placeholder {
  color: $brown;
}
:-moz-placeholder { /* Firefox 18- */
   color: $brown;
}
::-moz-placeholder {  /* Firefox 19+ */
   color: $brown; 
}
:-ms-input-placeholder {  
   color: $brown;
}

h1.page__title {
  font-family: $fancy-font;
  padding-top: 0.5em;
  text-shadow: 1px 1px 5px rgba(0,0,0,0.15);
}

/* header */
.header {
  text-align: center;
  
  .header__logo {
    display: inline-block;
    float: none;
    margin-top: 10em;
  
    &:hover {
      border-bottom: 1px solid transparent;
    }
  }

  .header__name-and-slogan {
    float: none;
    margin: 2em 0;
  
    .header__site-name {
      height: 0;
      text-indent: -20000px;
    }

    .header__site-slogan {
      font-family: $fancy-font;
      font-size: 2em;
      font-weight: 400;
      line-height: 1.15;
      text-shadow: 1px 1px 5px rgba(0,0,0,0.5);
      
      @media (max-width: $break-small) {
        font-size: 1.75em;
      }
    }
  }
}

/* navigation */
#navigation {
  background: $brown;
  height: auto;
  margin: 1em 0 0;
  padding: 2em 0;
  position: initial;
  top: auto;
  
  #main-menu > ul {
    display: block;
    margin: 0;
    padding: 0;
    text-align: center;
    
    li {
      display: inline-block;
      float: none;
      padding: 1em;
      
      a {
        display: inline-block;
        color: $white;
        font-size: 0.75em;
        font-family: $fancy-font;
        padding: 4.5em 0 0;
        -moz-transition: all 0.5s ease 0.1s;
        -o-transition: all 0.5s ease 0.1s;
        -webkit-transition: all 0.5s ease 0.1s;
        transition: all 0.5s ease 0.1s;
        width: 70px;
        
        &.nav-about {
          background: url('../images/nav-icons.png') no-repeat -350px 0;
          
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat -350px -95px;
          }
        }
        
        &.nav-blog {
          background: url('../images/nav-icons.png') no-repeat -70px 0;
        
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat -70px -95px;
          }
        }
        
        &.nav-community {
          background: url('../images/nav-icons.png') no-repeat 0 0;
        
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat 0 -95px;
          }
        }
        
        &.nav-contact {
          background: url('../images/nav-icons.png') no-repeat -140px 0;
        
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat -140px -95px;
          }
        }
        
        &.nav-gallery {
          background: url('../images/nav-icons.png') no-repeat -210px 0;
        
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat -210px -95px;
          }
        }
        
        &.nav-membership {
          background: url('../images/nav-icons.png') no-repeat -280px 0;
        
          &.active,
          &:hover,
          &:active {
            background: url('../images/nav-icons.png') no-repeat -280px -95px;
          }
        }
      }
    }
  }
}
#navigation #main-menu > ul li a.active.nav-contact {
  background: url('../images/nav-icons.png') no-repeat -140px 0;
}
#navigation #main-menu > ul li a.active.nav-contact:hover,
#navigation #main-menu > ul li a.active.nav-contact:active {
  background: url('../images/nav-icons.png') no-repeat -140px -95px;
}
.node-type-blog-entry #navigation #main-menu > ul li a.nav-blog,
.node-type-blog #navigation #main-menu > ul li a.nav-blog {
  background: url('../images/nav-icons.png') no-repeat -70px -95px;
}

/* content */
#page {
  background: $white;
  color: $brown;
  margin: 0;
}
#main {
  padding-top: 1em;
}
.front h1.page__title,
.front #first-time {
  display: none;
}
h2.block-title {
  font-family: $fancy-font;
  margin-bottom: 1em;
  text-align: center;
  
  @media all and (max-width: 600px) {
    font-size: 1.25em;
  }
}
.freetrial {
  border-bottom: 1px dashed $orange-opaque;
  margin-bottom: 1em;
  padding-bottom: 3em;
}
.latest-blog {
  background: $orange;
  margin-bottom: 0;
  overflow: hidden;
  padding: 1em 0;
  text-align: center;
  
  h2 {
    display: inline;
    margin: 0 0.5em 0 0;
  }
  .content,
  .view,
  .view-content,
  .views-row,
  .views-field {
    display: inline;
    margin: 0;
  }
  
  .views-field-title a {
    color: $white;
  }
  
  @media all and (max-width: 600px) {
    h2,
    .content {
      display: block;
    }
  }
}
.contact-block {
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(left,  rgba(255,255,255,0.01) 0%, rgba(255,255,255,0.6) 40%, rgba(255,255,255,0.75) 50%, rgba(255,255,255,0.6) 60%, rgba(255,255,255,0.01) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(255,255,255,0.01)), color-stop(40%,rgba(255,255,255,0.6)), color-stop(50%,rgba(255,255,255,0.75)), color-stop(60%,rgba(255,255,255,0.6)), color-stop(100%,rgba(255,255,255,0.01))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* IE10+ */
  background: linear-gradient(to right,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#03ffffff', endColorstr='#03ffffff',GradientType=1 ); /* IE6-8 */

  margin: 0;
  padding: 2em 0 0.5em;
  text-shadow: 1px 1px 5px rgba(0,0,0,0.25);
  
  > h2 {
    color: $brown;
    font-family: $fancy-font;
    font-size: 2em;
    margin: 0;
    text-align: center;
  }
  
  .node-webform {
    color: $brown;
      
    > header {
      display: none;
    }
    
    .field-name-body {
      font-size: 1.25em;
      
      p {
        margin: 0 0 0.5em;
      
        a {
          color: $brown;
          
          &:hover {
            border-bottom: 1px solid $brown;
          }
        }
      }
    }
  }
}
.contact-text {
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(left,  rgba(255,255,255,0.01) 0%, rgba(255,255,255,0.6) 40%, rgba(255,255,255,0.75) 50%, rgba(255,255,255,0.6) 60%, rgba(255,255,255,0.01) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(255,255,255,0.01)), color-stop(40%,rgba(255,255,255,0.6)), color-stop(50%,rgba(255,255,255,0.75)), color-stop(60%,rgba(255,255,255,0.6)), color-stop(100%,rgba(255,255,255,0.01))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(left,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* IE10+ */
  background: linear-gradient(to right,  rgba(255,255,255,0.01) 0%,rgba(255,255,255,0.6) 40%,rgba(255,255,255,0.75) 50%,rgba(255,255,255,0.6) 60%,rgba(255,255,255,0.01) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#03ffffff', endColorstr='#03ffffff',GradientType=1 ); /* IE6-8 */

  color: $black;
  font-size: 1.125em;
  margin: 0;
  padding: 0 0 1em;
  text-align: center;
  text-shadow: 1px 1px 5px rgba(0,0,0,0.25);
  
  p {
    margin: 0;
  }
}


.section-memberships .node {
  text-align: center;
  
  p {
    text-align: left;
  }
  
  ul {
    text-align: left;
  }
}
.membership-level {
  @include box-sizing(border-box);
  
  background: $orange-opaque;
  background-size: 100% !important;
  border: 1px solid $orange;
  border-radius: 100%;
  box-shadow: 1px 1px 5px rgba(0,0,0,0.25);
  display: inline-block;
  height: 240px;
  margin: 0 1em 2em;
  padding: 1em;
  text-align: center;
  width: 240px;
  vertical-align: top;
  
  &.membership-24 {background: $orange-opaque url('../images/membership_24.png') no-repeat 50% 50%;}
  &.membership-5 {background: $orange-opaque url('../images/membership_5.png') no-repeat 50% 50%;}
  &.membership-3 {background: $orange-opaque url('../images/membership_3.png') no-repeat 50% 50%;}
  &.membership-2 {background: $orange-opaque url('../images/membership_2.png') no-repeat 50% 50%;}
  &.membership-1 {background: $orange-opaque url('../images/membership_1.png') no-repeat 50% 50%;}
  &.membership-single {background: $orange-opaque url('../images/membership_1.png') no-repeat 50% 50%;}
  
  > h2 {
    font-family: $fancy-font;
    font-size: 1.5em;
    margin: 2em 0 0;
    
    @media all and (max-width: 650px) {
      font-size: 1em;
      margin-top: 0.5em;
    }
    
    @media all and (max-width: 400px) {
      margin-top: 0;
    }
  }
  
  > p {
    margin: 0;
    text-align: center !important;
    
    @media all and (max-width: 650px) {
      font-size: 0.75em;
    }
  }
  
  > h4 {
    color: $orange;
    font-family: $fancy-font;
    font-size: 1.75em;
    margin: 0 0 0.25em;
    text-shadow: 0 0 1px rgba(0,0,0,0.25);
    
    @media all and (max-width: 650px) {
      font-size: 1em;
    }
  }
  
  @media all and (max-width: 650px) {
    height: 150px;
    margin: 0 1em 1em;
    width: 150px;
  }
  
  @media all and (max-width: 400px) {
    height: 140px;
    margin: 0 0.25em 1em;
    width: 140px;
  }
}

/* responsive video */
.video-container {
	position: relative;
	padding-bottom: 56.25%;
	padding-top: 30px;
	height: 0;
	overflow: hidden;
  
  iframe,  
  object,  
  embed {
	  position: absolute;
	  top: 0;
	  left: 0;
	  width: 100%;
	  height: 100%;
  }
}

/* Views */
.view ul {
  margin: 0;
  padding: 0;
  
  li {
    list-style-type: none;
    margin: 0;
    padding: 0;
    
    .views-field-title {
      font-size: 1.25em;
      font-weight: bold;
    }
    .views-field-created {
      font-size: 0.85em;
    }
    p{
      margin: 0 0 1em;
    }
  }
}

.pager li {
  border: 1px solid $orange;
  line-height: 2em;
  margin: 0 0.25em;
  
  &.pager-current {
    background: $orange-opaque;
    font-weight: normal;
    padding: 0 0.5em;
    text-decoration: none;
  }
  &.pager-ellipsis {
    padding: 0 0.5em;
  }
  
  a {
    padding: 0 0.5em;
    
    &:hover {
      background: $orange-opaque;
      border-bottom: none;
    }
  }
}

.featured-member {
  @include box-sizing(border-box);
  
  margin: 0 auto 2em;
  max-width: 960px;
  overflow: hidden;
  padding: 0 1em;
  text-align: center;
  
  .block-title {
    margin: 0 0 1em;
  }
  
  .views-field-title {
    font-size: 1.5em;
    margin-bottom: 0.75em;
    margin-top: 0.75em;
  }
  
  @media all and (min-width: 400px) {
    text-align: left;
    
    .views-field-field-member-image {
      float: left;
      margin: 0 1em 1em 0;
    }
    .views-field-title {
      margin-top: 0;
    }
  }
}

.view-community {
  .views-row {
    @include box-sizing(border-box);
    
    float: left;
    min-height: 290px;
    padding: 1em;
    text-align: center;
    width: 33%;
    
    @media all and (max-width: 600px) {
      width: 50%;
    }
    
    .community-image-wrapper {
      height: 180px;
      margin: 0 auto;
      position: relative;
      width: 180px;
      
      @media all and (max-width: 480px) {
        height: 120px;
        width: 120px;
      }
      
      img {
        left: 0;
        position: absolute;
        top: 0;
      }
      
      .community-info {
        @include box-sizing(border-box);
        
        background: $black;
        background: rgba(0,0,0,0.7);
        color: $white;
        display: none;
        font-size: 0.875em;
        height: 100%;
        left: 0;
        padding: 1em;
        position: absolute;
        text-align: left;
        top: 0;
        width: 100%;
        
        @media all and (max-width: 480px) {
          font-size: 0.65em;
        }
      }
    
      &:hover,
      &:active {
        .community-info {
          display: block;
        }
      }
    }
    
    .views-field-title {
      font-size: 1.125em;
      font-weight: bold;
    }
  }
}

.view-gallery { 
  ul {
    overflow: hidden;
    width: 100%;
    
    li {
      @include box-sizing(border-box);
        
      float: left;
      min-height: 270px;
      padding: 1em;
      text-align: center;
      width: 33%;
      vertical-align: top;
      
      @media all and (max-width: 480px) {
        width: 50%;
      }
        
      p {
        margin: 0;
      }
        
      .views-field-title {
        color: $orange;
        
        @media all and (max-width: 480px) {
          font-size: 0.75em;
        }
      }
    }
  }
}

.node-member {
  .field-name-field-member-image {
    float: left;
    margin: 0 1em 1em 0;
    max-width: 100%;
    width: 300px;
  
    @media all and (max-width: 600px) {
      float: none;
      margin: 0 auto 1em;
    }
  }
  
  .field-name-body {
    margin-bottom: 1em;
  }
  
  .field-name-field-twitter-handle {
    margin: 1em 0 0;
  }
}


/* Footer */
.front #footer {
  background: url('../images/map.jpg') no-repeat 50% 50%;
  background-size: cover;
  padding: 0 0 0 0;
}

/* copyright */
.copyright {
  background: #fff;
  color: rgb(70,53,47);
  font-family: $fancy-font;
  font-size: 1.25em;
  font-weight: 700;
  padding: 3em 1em;
  text-align: center;
}

#social {
  margin: 1em 0 0;
  
  a {
    display: inline-block;
    height: 50px;
    margin: 0 1em;
    text-indent: -20000px;
    width: 50px;
    
    &.tw {
      background: url('../images/social.png') no-repeat 0 0;
    }
    &.fb {
      background: url('../images/social.png') no-repeat -2.5em 0;
    }
    @media all and (min-width: 600px) {
      &:hover {
        background-position-y: -2.5em;
      }
    }
  }
  
